in=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12\">\n                <h3 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>The only complete CRM for Indian</b><br><b> businesses!\n</b><div><b><br></b></div></h3>\n                <p class=\"mbr-text mbr-fonts-style mb-4\" mbr-theme-style=\"display-1\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">\n                    Create your own landing page today.\n                </p>\n                <div class=\"link-wrap\">\n                    <p class=\"link mbr-fonts-style mb-4\" mbr-theme-style=\"display-1\" mbr-if=\"showLink\" data-app-selector=\".link\">\n                        <a class=\"text-primary\" href=\"#\"><b>No Coding &gt;&gt;</b></a>\n                    </p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
